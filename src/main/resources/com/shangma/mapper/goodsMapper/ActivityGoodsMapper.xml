<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shangma.mapper.goodsMapper.ActivityGoodsMapper">

    <select id="search" resultType="com.shangma.entity.goodsEntity.ActivityGoods">
        <where>
            <if test="freeGoodsName != null and freeGoodsName != '' ">
                free_goods_name like "%${freeGoodsName}%"
            </if>
        </where>
    </select>
</mapper>