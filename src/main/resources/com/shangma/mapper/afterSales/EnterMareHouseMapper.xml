<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shangma.mapper.afterSales.EnterMareHouseMapper">

    <select id="selectBySearch" resultType="com.shangma.entity.afterSales.EnterMareHouse">
        select * from enter_marehouse
        <where>
            <if test="id!=null and id!=''">
                id = '${id}'
            </if>
            <if test="sign!=null and sign!=''">
                and sign like '%${sign}%'
            </if>
            <if test="producer!=null and producer!=''">
                and producer like '%${producer}%'
            </if>
            <if test="startTime !=null and startTime != ''">
                and produce_time &gt;='${startTime}'
            </if>
            <if test="endTime != null and endTime != ''">
                and produce_time &lt;='${endTime}'
            </if>
            <if test="approver !=null and approver != ''">
                and approver like '%${approver}%'
            </if>
            <if test="approverStatus != null and approverStatus != ''">
                and approverStatus like '%${approverStatus}%'
            </if>
        </where>


    </select>




</mapper>
